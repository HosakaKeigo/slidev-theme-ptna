{
  "name": "slidev-theme-ptna",
  "version": "0.1.7",
  "description": "PTNA (全日本ピアノ指導者協会) theme for Slidev",
  "author": "Hosaka Keigo <hosaka@piano.or.jp>",
  "license": "MIT",
  "homepage": "https://www.piano.or.jp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HosakaKeigo/slidev-theme-ptna.git"
  },
  "scripts": {
    "format": "prettier --write ."
  },
  "keywords": [
    "slidev-theme",
    "slidev",
    "ptna",
    "piano"
  ],
  "engines": {
    "node": ">=20.0.0",
    "slidev": ">=v0.47.0"
  },
  "slidev": {
    "defaults": {
      "fonts": {
        "mono": "Fira Code",
        "sans": "Hiragino Kaku Gothic ProN, Hiragino Sans, Yu Gothic UI, Meiryo UI, sans-serif",
        "serif": "Hiragino Mincho ProN, Yu Mincho, serif",
        "local": "Hiragino Kaku Gothic ProN"
      },
      "highlighter": "shiki",
      "colorSchema": "light",
      "routerMode": "history",
      "aspectRatio": "16/9",
      "canvasWidth": 1920,
      "background": "#ffffff"
    }
  },
  "dependencies": {
    "@slidev/client": "^52.1.0",
    "@slidev/types": "^0.47.0",
    "codemirror-theme-vars": "^0.1.2",
    "echarts": "^5.6.0",
    "vue": "^3.5.21",
    "vue-echarts": "^7.0.3"
  },
  "devDependencies": {
    "@slidev/cli": "^52.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "files": [
    "README.md",
    "components",
    "layouts",
    "assets",
    "styles",
    "setup",
    "layoutHelper.ts",
    "index.ts"
  ]
}